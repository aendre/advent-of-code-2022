{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "airbnb-base",
      "airbnb-typescript/base",
      "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.eslint.json"
    },
    "rules": {
      "no-console": "error",
      "import/prefer-default-export" : "off"
    }
}
